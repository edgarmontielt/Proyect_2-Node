<main class="flex gap-5 m-16">
  <section class="w-1/3 bg-white rounded-md">
    {{#each user}}
      <div class="flex p-5 flex-col justify-center w-auto">
        <img
          src="{{this.profile_pic}}"
          alt="{{this.username}}"
          class="w-16 h-16 rounded-2xl"
        />
        <div>
          <p class="text-lg font-semibold">{{this.username}}</p>
          <p>{{this.name}}</p>
          <p>{{this.email}}</p>
        </div>
      </div>
    {{/each}}
  </section>
  <section class="w-2/3 bg-white rounded-md">

  </section>
</main>