{{#if session.user.loggedIn}}
  {{#if hasUsers}}
    <section class="w-screen h-16 flex justify-center">
      <div
        class="w-1/4 bg-white rounded-b-md py-5 px-10 flex flex-row gap-4 items-center"
      >
        <img
          src="{{user.profile_pic}}"
          alt=""
          class="w-8 h-8 rounded-full mt-[1px]"
        />
        <p class="font-medium">{{user.name}}</p>
      </div>
    </section>
  {{/if}}
  <main class="flex gap-5 m-16">
    <section class="w-1/3 bg-white rounded-md flex justify-center">
      <div class="flex p-5 flex-col w-auto items-center">
        <img
          src="{{session.user.profilePic}}"
          alt=""
          class="w-36 h-36 rounded-full object-cover"
        />
        <div class="text-center mt-4">
          <p class="text-xl font-semibold">{{session.user.name}}</p>
          <p class="text-xs text-gray-500">{{session.user.occupation}}</p>
          {{!-- <p> <strong>Fecha de nacimiento: </strong>{{formatDate session.user.birthday}}</p> --}}
        </div>
      </div>
    </section>
    <section class="w-2/3 bg-white rounded-md">
      {{!-- {{user.name}} --}}
    </section>
  </main>
{{/if}}