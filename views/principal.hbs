{{#if session.user.loggedIn}}
<main class="flex gap-5 m-16">
  <section class="w-1/3 bg-white rounded-md flex justify-center">
      <div class="flex p-5 flex-col w-auto items-center">
        <img
          src="{{session.user.profilePic}}"
          alt=""
          class=" w-36 h-36 rounded-xl"
        />
        <div class=" text-center mt-4">
          <p class="text-xl font-semibold">{{session.user.name}}</p>
          <p class=" text-xs text-gray-500">{{session.user.username}}</p>
          {{!-- <p> <strong>Fecha de nacimiento: </strong>{{formatDate session.user.birthday}}</p> --}}
        </div>
      </div>
  </section>
  <section class="w-2/3 bg-white rounded-md">

  </section>
</main>
{{/if}}